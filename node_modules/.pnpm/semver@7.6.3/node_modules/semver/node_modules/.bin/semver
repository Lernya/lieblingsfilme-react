#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/linuxdata/dlerny/webdev/code/primer/d2409-lieblingsfilme/gitclone-lieblingsfilme-react/node_modules/.pnpm/semver@7.6.3/node_modules/semver/bin/node_modules:/linuxdata/dlerny/webdev/code/primer/d2409-lieblingsfilme/gitclone-lieblingsfilme-react/node_modules/.pnpm/semver@7.6.3/node_modules/semver/node_modules:/linuxdata/dlerny/webdev/code/primer/d2409-lieblingsfilme/gitclone-lieblingsfilme-react/node_modules/.pnpm/semver@7.6.3/node_modules:/linuxdata/dlerny/webdev/code/primer/d2409-lieblingsfilme/gitclone-lieblingsfilme-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/linuxdata/dlerny/webdev/code/primer/d2409-lieblingsfilme/gitclone-lieblingsfilme-react/node_modules/.pnpm/semver@7.6.3/node_modules/semver/bin/node_modules:/linuxdata/dlerny/webdev/code/primer/d2409-lieblingsfilme/gitclone-lieblingsfilme-react/node_modules/.pnpm/semver@7.6.3/node_modules/semver/node_modules:/linuxdata/dlerny/webdev/code/primer/d2409-lieblingsfilme/gitclone-lieblingsfilme-react/node_modules/.pnpm/semver@7.6.3/node_modules:/linuxdata/dlerny/webdev/code/primer/d2409-lieblingsfilme/gitclone-lieblingsfilme-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/semver.js" "$@"
else
  exec node  "$basedir/../../bin/semver.js" "$@"
fi
